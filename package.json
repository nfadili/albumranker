{
    "name": "albumranker",
    "private": true,
    "license": "MIT",
    "sideEffects": false,
    "scripts": {
        "postinstall": "remix setup node",
        "build": "run-s build:*",
        "build:remix": "remix build",
        "dev": "run-p dev:*",
        "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
        "format": "prettier --write .",
        "lint": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
        "setup": "prisma db push",
        "start": "remix-serve build",
        "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
        "test": "vitest",
        "typecheck": "tsc -b",
        "validate": "run-p \"test -- --run\" lint typecheck"
    },
    "eslintIgnore": [
        "/node_modules",
        "/build",
        "/public/build"
    ],
    "dependencies": {
        "@mantine/core": "^4.1.5",
        "@mantine/hooks": "^4.1.5",
        "@mantine/notifications": "^4.2.10",
        "@mantine/ssr": "^4.1.5",
        "@prisma/client": "^3.11.1",
        "@remix-run/node": "^1.6.1",
        "@remix-run/react": "^1.6.1",
        "@remix-run/serve": "^1.6.1",
        "@remix-run/server-runtime": "^1.6.1",
        "bcryptjs": "^2.4.3",
        "classnames": "^2.3.1",
        "react": "^17.0.2",
        "react-beautiful-dnd": "^13.1.0",
        "react-dom": "^17.0.2",
        "spotify-web-api-node": "^5.0.2",
        "tabler-icons-react": "^1.46.0",
        "tiny-invariant": "^1.2.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^6.1.1",
        "@remix-run/dev": "^1.4.1",
        "@remix-run/eslint-config": "^1.6.3",
        "@testing-library/dom": "^8.12.0",
        "@testing-library/jest-dom": "^5.16.3",
        "@testing-library/react": "^12.1.4",
        "@testing-library/user-event": "^14.0.4",
        "@types/bcryptjs": "^2.4.2",
        "@types/eslint": "^8.4.1",
        "@types/lodash.debounce": "^4.0.7",
        "@types/node": "^17.0.23",
        "@types/react": "^17.0.43",
        "@types/react-beautiful-dnd": "^13.1.2",
        "@types/react-dom": "^17.0.14",
        "@types/react-table": "^7.7.11",
        "@types/spotify-web-api-node": "^5.0.7",
        "@vitejs/plugin-react": "^1.3.0",
        "autoprefixer": "^10.4.4",
        "binode": "^1.0.5",
        "c8": "^7.11.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "happy-dom": "^2.55.0",
        "msw": "^0.39.2",
        "npm-run-all": "^4.1.5",
        "prettier": "2.7.1",
        "prisma": "^3.11.1",
        "start-server-and-test": "^1.14.0",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.14.1",
        "typescript": "^4.6.3",
        "vite": "^2.9.1",
        "vite-tsconfig-paths": "^3.5.0",
        "vitest": "^0.8.2"
    },
    "engines": {
        "node": ">=16"
    },
    "prisma": {
        "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
    }
}
