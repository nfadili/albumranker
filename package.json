{
    "name": "albumranker",
    "private": true,
    "license": "MIT",
    "sideEffects": false,
    "scripts": {
        "postinstall": "remix setup node",
        "build": "run-s build:*",
        "build:remix": "remix build",
        "db:generate": "prisma generate",
        "dev": "run-p dev:*",
        "dev:remix": "cross-env NODE_ENV=development binode -- @remix-run/dev:remix dev",
        "format": "prettier --write .",
        "lint": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
        "setup": "prisma db push",
        "start": "remix-serve build",
        "typecheck": "tsc -b",
        "validate": "run-p lint typecheck"
    },
    "eslintIgnore": [
        "/node_modules",
        "/build",
        "/public/build"
    ],
    "dependencies": {
        "@emotion/cache": "^11.10.5",
        "@emotion/react": "^11.10.5",
        "@emotion/serialize": "^1.1.1",
        "@emotion/server": "^11.10.0",
        "@mantine/core": "^5.9.1",
        "@mantine/hooks": "^5.9.1",
        "@mantine/notifications": "^5.9.1",
        "@mantine/remix": "^5.9.1",
        "@prisma/client": "^4.7.1",
        "@remix-run/node": "^1.9.0",
        "@remix-run/react": "^1.6.1",
        "@remix-run/serve": "^1.6.1",
        "@remix-run/server-runtime": "^1.6.1",
        "bcryptjs": "^2.4.3",
        "classnames": "^2.3.1",
        "react": "^17.0.2",
        "react-beautiful-dnd": "^13.1.0",
        "react-dom": "^17.0.2",
        "spotify-web-api-node": "^5.0.2",
        "tabler-icons-react": "^1.46.0",
        "tiny-invariant": "^1.2.0"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.9.0",
        "@remix-run/eslint-config": "^1.4.1",
        "@types/bcryptjs": "^2.4.2",
        "@types/eslint": "^8.4.1",
        "@types/lodash.debounce": "^4.0.7",
        "@types/node": "^20.3.3",
        "@types/react": "^17.0.43",
        "@types/react-beautiful-dnd": "^13.1.2",
        "@types/react-dom": "^17.0.14",
        "@types/react-table": "^7.7.11",
        "@types/spotify-web-api-node": "^5.0.7",
        "@vitejs/plugin-react": "^1.3.0",
        "autoprefixer": "^10.4.4",
        "binode": "^1.0.5",
        "cross-env": "^7.0.3",
        "eslint": "^8.31.0",
        "eslint-config-prettier": "^8.5.0",
        "npm-run-all": "^4.1.5",
        "prettier": "2.7.1",
        "prisma": "^4.12.0",
        "run-p": "^0.0.0",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.14.1",
        "typescript": "^4.6.3",
        "vite": "^2.9.1",
        "vite-tsconfig-paths": "^3.5.0"
    },
    "engines": {
        "node": ">=16"
    }
}
